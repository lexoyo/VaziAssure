{if(!(ext&&/^(jpg|png|jpeg|gif)$/i.test(ext)))
{var errormsg="Format invalide";if($("#add_ajax_image_error"))
{var errormsg=$("#add_ajax_image_error").text();}
alert(errormsg);return false;}
else
{this.disable();progress.show();}},onComplete:function(file,response)
{this.enable();progress.hide();thisimageid=response.substring(0,response.indexOf(':'));thisimageurl=response.substring(response.indexOf(':')+1);$("#preview_ajax_image").attr("src",thisimageurl);$("#id_ajax_image").val(thisimageid);return true;}});}
