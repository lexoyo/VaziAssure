{var submenu=$('#co_user_profile > ul');submenu.removeClass('hover');});$('#co_user_profile > ul a:last').bind('blur',function(e)
{var submenu=$('#co_user_profile > ul');submenu.removeClass('hover');});$('.co_slideshow').shuffle();$('.co_slideshow').cycle({timeout:5000,speed:'slow',prev:'#co_slideshow_prev',next:'#co_slideshow_next',slideExpr:'.co_slide'});$("#comuto_date_debut, #comuto_date_fin").datepicker({showOn:'both',showOtherMonths:true,showWeeks:false,firstDay:1,changeFirstDay:false,buttonImageOnly:true,dateFormat:'d M yy',onSelect:function(date)
{var dateval=$.datepicker.formatDate("dd/mm/yy",$("#"+this.id).datepicker("getDate"));$("#"+this.id).val(dateval);$("#"+this.id).attr("value",dateval);},buttonImage:'/static/images/calendar-dp.png'});if($("#add_ajax_image").length)
{var progress=$("#id_ajax_image_progress").hide();var typePhoto=$("#id_photo_type").val();if(!typePhoto){typePhoto='MAIN';}
